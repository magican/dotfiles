          "settings": {
            "foreground": "#71649b"
          }
        },
        {
          "scope": "support.type.property-name.json.comments",
          "settings": {
            "foreground": "#b063f0"
          }
        },
        {
          "scope": "meta.function-call.generic.python, entity.name.function.ts, new.expr.ts entity.name.type.ts",
          "settings": {
            "foreground": "#76c7ae"
          }
